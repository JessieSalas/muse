<SidePanel>:

    size_hint: .675,.75
    pos_hint: {'x': 0.165, 'y': .12}
    orientation: 'vertical'
    spacing: 1

    MenuItem:
        text: 'Search'

    MenuItem:
        text: 'Saved Searches'

    MenuItem:
        text: 'My Patents'

    MenuItem:
        text: 'Settings'

<AppActionBar>:

    size_hint: .675,.75
    pos_hint: {'x': 0.165, 'y': .12}
    pos_hint: {'top':1}
    ActionView:
        use_separator: True
        ActionMenu:
            font_name: 'assets/Josef-Regular.ttf'
            title: app.navbar_label
            with_previous: False
            app_icon: 'assets/muse_logo.png'
            mipmap: True
            #icon: 'tent_logo.png'
            on_press: self.menu()
